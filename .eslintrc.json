{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-console":["warn"],
        "no-unused-vars":["warn"],
        "no-undef":["warn"],
        "no-redeclare":["warn"],
        "no-unreachable":["warn"],
        "no-mixed-spaces-and-tabs":["warn"],
        "no-self-assign":["warn"],
        "no-cond-assign":["warn"],
        "no-case-declarations":["warn"],
        "no-dupe-class-members":["warn"],
        "no-empty":["warn"],
        "valid-typeof":["warn"]
    }
}